{
  "name": "@hit/feature-pack-metrics-core",
  "version": "1.0.51",
  "description": "A HIT feature pack",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./pages/*": {
      "types": "./dist/pages/*.d.ts",
      "import": "./dist/pages/*.js",
      "require": "./dist/pages/*.js",
      "default": "./dist/pages/*.js"
    },
    "./schema": {
      "types": "./dist/schema/metrics-core.d.ts",
      "import": "./dist/schema/metrics-core.js",
      "require": "./dist/schema/metrics-core.js",
      "default": "./dist/schema/metrics-core.js"
    },
    "./server/api/*": {
      "types": "./dist/server/api/*.d.ts",
      "import": "./dist/server/api/*.js",
      "require": "./dist/server/api/*.js",
      "default": "./dist/server/api/*.js"
    },
    "./server/lib/*": {
      "types": "./dist/server/lib/*.d.ts",
      "import": "./dist/server/lib/*.js",
      "require": "./dist/server/lib/*.js",
      "default": "./dist/server/lib/*.js"
    },
    "./cli/*": {
      "types": "./dist/cli/*.d.ts",
      "import": "./dist/cli/*.js",
      "require": "./dist/cli/*.js",
      "default": "./dist/cli/*.js"
    }
  },
  "files": [
    "dist",
    "feature-pack.yaml"
  ],
  "scripts": {
    "clean": "rm -rf dist",
    "build": "npm run clean && tsc",
    "dev": "tsc --watch",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "js-yaml": "^4.1.1",
    "pg": "^8.16.3"
  },
  "peerDependencies": {
    "@hit/ui-kit": "*",
    "react": "^18.0.0",
    "next": "^14.0.0",
    "@hit/sdk": "*",
    "zod": ">=3.22.0",
    "lucide-react": ">=0.263.0",
    "drizzle-orm": ">=0.30.0"
  },
  "devDependencies": {
    "@hit/sdk": "github:c0x65o/hit-sdk-typescript",
    "@hit/ui-kit": "github:c0x65o/hit-ui-kit",
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.0",
    "@types/js-yaml": "^4.0.9",
    "@types/pg": "^8.15.6",
    "drizzle-orm": "^0.30.0",
    "zod": "^3.22.0",
    "lucide-react": "^0.460.0",
    "typescript": "^5.0.0",
    "next": "^14.2.5"
  },
  "keywords": [
    "hit",
    "feature-pack",
    "metrics-core"
  ],
  "license": "MIT"
}
